{
  "name": "pug",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "tag"
      }
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tag_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "inline_text"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "tag_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_-]+"
    },
    "inline_text": {
      "type": "PATTERN",
      "value": ".+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

